# FloatChat Cloud Environment Configuration

# Application Settings
APP_NAME=FloatChat
APP_VERSION=1.0.0
DEBUG=false

# Cloud Database (Supabase PostgreSQL)
DATABASE_URL=postgresql://postgres:your_password@your_supabase_url:5432/postgres

# Cloud AI Services
OPENAI_API_KEY=sk-proj-your_openai_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
GROQ_API_KEY=gsk_your_groq_api_key_here

# Vector Database (Pinecone)
PINECONE_API_KEY=pcsk_your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-east-1
PINECONE_INDEX_NAME=floatchat-knowledge

# No file storage needed - everything processed in memory

# Redis Cloud for caching (optional)
REDIS_URL=redis://default:your_password@your_redis_url:port

# Data Sources (Public APIs - No Registration Required)
ARGO_API_BASE=https://data-argo.ifremer.fr
ERDDAP_BASE=https://www.ifremer.fr/erddap
COPERNICUS_MARINE_BASE=https://marine.copernicus.eu
NOAA_ERDDAP_BASE=https://coastwatch.pfeg.noaa.gov/erddap
